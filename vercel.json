{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/logo(.*).png",
      "dest": "/logo$1.png"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_FIREBASE_API_KEY": "AIzaSyDHpXWODJ6wQ6XIPVMlicJduVc3IxbeC4Q",
    "REACT_APP_FIREBASE_AUTH_DOMAIN": "groundorg.firebaseapp.com",
    "REACT_APP_FIREBASE_PROJECT_ID": "groundorg",
    "REACT_APP_FIREBASE_STORAGE_BUCKET": "groundorg.firebasestorage.app",
    "REACT_APP_FIREBASE_MESSAGING_SENDER_ID": "1029639411235",
    "REACT_APP_FIREBASE_APP_ID": "1:1029639411235:web:8f5c2598ecb509e2060396",
    "REACT_APP_FIREBASE_MEASUREMENT_ID": "G-LV6QY2TB9R",
    "SKIP_PREFLIGHT_CHECK": "true",
    "TSC_COMPILE_ON_ERROR": "true",
    "ESLINT_NO_DEV_ERRORS": "true",
    "DISABLE_ESLINT_PLUGIN": "true"
  }
} 